pipeline{
    agent any
    stages {
        stage ('Cleanup') {
            steps {
                deleteDir()
            }            
        
        stage ('Clone') {
               steps {
                   git clone /1.3/MICA_L0/Test1/
               }
        }       
        stage ('Build') {
            steps {
                bat "dir.bat"
            }
        }
   
    }
    
}
