pipeline{
    agent any
    stages {
        stage ('Cleanup') {
            steps {
                deleteDir()
            }            
        }
        stage ('Clone Repo'){
            steps {
                dir('prototype2') {
                    sh "git clone https://github.com/jjinnm4real/testRepo.git ."
                }    
            }
        }
        stage ('Build') {
            steps {
                dir('prototype2') {
                    bat "dir.bat"
                    

                }    
            }
        }
        
    }
    
}    
